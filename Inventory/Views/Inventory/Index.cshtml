<h1>Collection</h1>
<a href='/items/new'>Add a New Card<a/>
<a href='/'>Back to Home<a/>
<table>
  <tr>
    <th>Pokemon Name</th>
    <th>Type</th>
    <th>Number</th>
  </tr>
  @foreach(var item in Model)
  {
  <tr>
    <td>@item.GetName()</td>
    <td>@item.GetPokemonType()</td>
    <td>@item.GetNumber()</td>
  </tr>
  }
</table>
